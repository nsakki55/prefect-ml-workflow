{
  "executionRoleArn": "arn:aws:iam::*****:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "prefect-flow",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs-agent"
        }
      },
      "portMappings": [],
      "command": [
        "prefect",
        "agent",
        "ecs",
        "start",
        "--launch-type",
        "EC2"
      ],
      "cpu": 512,
      "environment": [
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "ap-northeast-1"
        },
        {
          "name": "PREFECT__CLOUD__AGENT__LABELS",
          "value": "['production']"
        },
        {
          "name": "PREFECT__CLOUD__AGENT__LEVEL",
          "value": "INFO"
        },
        {
          "name": "PREFECT__CLOUD__API",
          "value": "https://api.prefect.io"
        },
        {
          "name": "PREFECT__CLOUD__API_KEY",
          "value": "pcs_*****"
        },
        {
          "name": "SYSTEM_ENV",
          "value": "production"
        },
        {
          "name": "SYSTEM_REGION",
          "value": "jp"
        }
      ],
      "memory": 1024,
      "image": "prefecthq/prefect:1.2.0-python3.8",
      "name": "prefect-ecs-agent"
    }
  ],
  "taskRoleArn": "arn:aws:iam::*****:role/prefect-task-role",
  "family": "prefect-ecs-agent-task",
  "networkMode": "awsvpc"
}